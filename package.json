{
  "name": "giterator",
  "description": "Clean up your Git history",
  "version": "0.5.3",
  "author": "Jamie Mason <jamie@foldleft.io> (https://github.com/JamieMason)",
  "bin": {
    "giterator": "dist/bin.js",
    "giterator-ungraph": "dist/bin-ungraph.js"
  },
  "bugs": "https://github.com/JamieMason/giterator/issues",
  "dependencies": {
    "bluebird": "3.5.0",
    "chalk": "2.1.0",
    "commander": "2.11.0",
    "execa": "0.8.0",
    "logservable": "1.0.0",
    "nodent-runtime": "3.2.1",
    "rimraf": "2.6.1",
    "tar": "4.0.1",
    "xstream": "10.9.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "eslint-config-xo": "0.26.0",
    "eslint-plugin-unicorn": "10.0.0",
    "fast-async": "6.3.0",
    "xo": "0.24.0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/JamieMason/giterator#readme",
  "keywords": [
    "git",
    "git-history",
    "git-log",
    "git-merge",
    "git-rebase",
    "log",
    "merge",
    "rebase"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "JamieMason/giterator",
  "resolutions": {
    "lodash": ">=4.17.15"
  },
  "scripts": {
    "build": "babel --out-dir ./dist --ignore node_modules ./src",
    "lint": "xo --fix",
    "prebuild": "rimraf dist",
    "prepack": "npm run build",
    "test": "npm run lint"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "esnext": true,
    "rules": {
      "import/extensions": 0,
      "import/no-unassigned-import": 0,
      "object-curly-spacing": 0,
      "unicorn/no-process-exit": 0
    },
    "space": 2
  }
}
